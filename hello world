package s1.helloworld;

/*
 * @author Chuck
 */
 
public class S1HelloWorld {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {

        String word;
        String finally;
        word = "figured";
        finally = String.format("I finally %s this out! I think?", word)
        System.out.println("Hello World!");
        System.out.println("The sum of two and three is " + "five");
        System.out.println("The sum of seven and eight is " + (7+8));
        System.out.println(finally);
      }
    }
